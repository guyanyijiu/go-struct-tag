{
	"name": "go-struct-tag",
	"displayName": "Go struct tag",
	"description": "Autocomplete go struct tag",
	"keywords": [
		"go",
		"golang",
		"struct",
		"tag",
		"complete",
		"json",
		"autocomplete"
	],
	"version": "0.3.0",
	"publisher": "liuchao",
	"repository": {
		"type": "git",
		"url": "https://github.com/guyanyijiu/go-struct-tag"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Programming Languages",
		"Extension Packs"
	],
	"activationEvents": [
		"onLanguage:go"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Go struct tag",
				"properties": {
					"go-struct-tag.cases": {
						"type": "array",
						"default": [
							"snake",
							"camel"
						],
						"description": "Supported: snake, camel, pascal, constant, none"
					},
					"go-struct-tag.customTags": {
						"type": "object",
						"default": {},
						"description": "Custom tags. Reference https://github.com/guyanyijiu/go-struct-tag#configuration"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"camel-case": "^4.1.2",
		"constant-case": "^3.0.4",
		"pascal-case": "^3.1.2",
		"snake-case": "^3.0.4"
	}
}
